    "husky": "4.3.8",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5"
  },
  "license": "MIT"
}
